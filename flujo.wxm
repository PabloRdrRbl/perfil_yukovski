imagpart(((-8-3*%i)-t0)*%e^-(%i*alpha))=p(-8,-3)*sin(theta(-8,-3)-alpha);
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
imagpart((a**2)/((-8-3*%i)-t0)*%e**(%i*alpha))=(a**2)/(p(-8,-3))*sin(alpha-theta(-8,-3));
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
imagpart(%i*T/(2*%pi)*log((-8-3*%i)-t0))=T/(2*%pi)*log(p(-8,-3));
/* [wxMaxima: input   end   ] */

